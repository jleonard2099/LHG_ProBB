   DIM TM$(27),DY$(7),NG%(18),Q$(25)
   DATA SUNDAY,MONDAY,TUESDAY,WEDNESDAY,THURSDAY,FRIDAY,SATURDAY
   FOR I=1 TO 7:READ DY$(I):NEXT
   DATA ATLANTA,BOSTON,CHARLOTTE,CHICAGO,CLEVELAND,DALLAS,DENVER
   DATA DETROIT,GOLDEN STATE,HOUSTON,INDIANA,LA CLIPPERS,LA LAKERS,MIAMI
   DATA MILWAUKEE,MINNESOTA,NEW JERSEY,NEW YORK,ORLANDO,PHILADELPHIA,PHOENIX
   DATA PORTLAND,SACRAMENTO,SAN ANTONIO,SEATTLE,UTAH,WASHINGTON
   FOR I=1 TO 27:READ TM$(I):NEXT
   YR%=92
   OPEN "NBASCH93.TXT" FOR INPUT AS #1
10 LINE INPUT #1, X$
   PRINT X$
   IF X$="" THEN 10
   FOR X=1 TO 7
   IF UCASE$(LEFT$(X$,LEN(DY$(X))))=DY$(X) THEN GOSUB 500:GOTO 10
   NEXT

20 FOR X=1 TO 27
   IF UCASE$(LEFT$(X$,LEN(TM$(X))))=TM$(X) THEN V$=TM$(X):V%=X:GOTO 30
   NEXT
   GOTO 10
30 X$=RIGHT$(X$,LEN(X$)-LEN(TM$(X))-1)
   L%=4
   IF UCASE$(LEFT$(X$,2))="AT" THEN L%=3
   X$=RIGHT$(X$,LEN(X$)-L%)
   PRINT "VIS="V$,V%
40 FOR X=1 TO 27
   IF UCASE$(LEFT$(X$,LEN(TM$(X))))=TM$(X) THEN H$=TM$(X):H%=X:GOTO 50
   NEXT
   PRINT "COULD NOT FULLY PROCESS "X$
   PRINT "DO YOU WISH TO CONTINUE"
   REM: HAVE LOOP TO READ NEXT LINE OR EXIT OUT
   END
50 X$=RIGHT$(X$,LEN(X$)-LEN(TM$(X))-2)
   PRINT "HOM="H$,H%
   PRINT X$
   IF LEN(X$)=9 THEN HR%=VAL(LEFT$(X$,1))+12:MIN%=VAL(MID$(X$,3,2))
   IF LEN(X$)=10 THEN HR%=VAL(LEFT$(X$,2))+12:MIN%=VAL(MID$(X$,4,2))
   IF HR%=24 THEN HR%=12
   NG%(15)=HR%*100+MIN%
   REM:WRITE THE GAME HERE
   GP%=GP%+1:PRINT "TOTAL="GP%
   NG%(8)=3
   YN$(0)="93":YN$(1)="93"
   NG%(0)=V%:NG%(1)=H%:NG%(2)=1:NG%(3)=1
   NG%(4)=0:NG%(5)=0:NG%(6)=2:NG%(7)=0
   NG%(9)=0:NG%(10)=0:NG%(11)=0:NG%(13)=0
   HR%=INT(NG%(15)/100)
   MIN%=NG%(15)-HR%*100
   TM%=HR%*60+MIN%
   NG%(12)=INT(TM%/30-18)
   NG%(14)=4
   NG%(17)=MO%
   NG%(18)=YR%
   NG%(16)=DY%
   OPEN "NBA92-93.SCD" AS #2 LEN=48
   FOR X=0 TO 18
   FIELD #2,X*2 AS X$,2 AS Q$(X+1),48-2-2*X AS X$
   NEXT
   FIELD #2,38 AS X$,2 AS Q$(22),2 AS Q$(23),3 AS Q$(20),3 AS Q$(21)
   FOR X=0 TO 18:LSET Q$(X+1)=MKI$(NG%(X)):NEXT
   FOR X=0 TO 1:LSET Q$(20+X)=YN$(X):NEXT
   LSET Q$(22)=MKI$(VS%):LSET Q$(23)=MKI$(HS%)
   PUT #2,GP%
   CLOSE 2
   GOTO 10
   END
500 L%=LEN(X$)
    X$=RIGHT$(X$,L%-LEN(DY$(X))-2)
    M$=LEFT$(X$,3)
    IF UCASE$(M$)="NOV" THEN MO%=11
    IF UCASE$(M$)="DEC" THEN MO%=12
    IF UCASE$(M$)="JAN" THEN MO%=1:YR%=93
    IF UCASE$(M$)="FEB" THEN MO%=2:YR%=93
    IF UCASE$(M$)="MAR" THEN MO%=3:YR%=93
    IF UCASE$(M$)="APR" THEN MO%=4:YR%=93
    DY%=VAL(RIGHT$(X$,2))
    RETURN
