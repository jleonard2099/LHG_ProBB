     IF H = 4 AND T >= 120 AND I5 > 10 AND I5 < 20 THEN D2(P9) = 3: P7(P9) = 1
     IF H = 4 AND T >= 120 AND I5 <= 10 THEN D2(P9) = 1: P7(P9) = 1
     IF H = 4 AND T < 120 AND I5 > 10 THEN D2(P9) = 1: P7(P9) = 0
