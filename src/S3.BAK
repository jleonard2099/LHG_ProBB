SUB MADEFT ()
     A(P, B1(P, B), 10) = A(P, B1(P, B), 10) + 1
     IF U6 <> 0 THEN
        LOCATE F3 + 9, 1
        CALL MOREDELAY
        PRINT F3; " GOOD"
