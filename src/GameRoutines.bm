'----------------------------------------
'           BOX2SPC Subroutine
'----------------------------------------
' Adds special event information
' to a boxscore file
Sub BOX2SPC (NM$)

    Open diskPaths$(1) + NM$ For Append As #2

    Print #2,

    'Output any special events
    If H - 1 > E%(1) Then Print #2, Using "MORE THAN ## QUARTERS"; E%(1)
    If score(1, 0) + score(0, 0) > E%(2) Then Print #2, Using "MORE THAN ### COMBINED POINTS"; E%(2)
    If R3(0) + R3(1) > E%(3) Then Print #2, Using "MORE THAN ### COMBINED REBOUNDS"; E%(3)
    If score(0, 0) > E%(4) Then Print #2, gameTeams$(0);: Print #2, Using " SCORED MORE THAN ### POINTS"; E%(4)
    If score(1, 0) > E%(4) Then Print #2, gameTeams$(1);: Print #2, Using " SCORED MORE THAN ### POINTS"; E%(4)
    If R3(0) > E%(5) Then Print #2, gameTeams$(0);: Print #2, Using " HAD MORE THAN ### REBOUNDS"; E%(5)
    If R3(1) > E%(5) Then Print #2, gameTeams$(1);: Print #2, Using " HAD MORE THAN ### REBOUNDS"; E%(5)

    For I = 0 To 1
        For I1 = 0 To 14
            If A!(I, I1, 8) * 2 + A!(I, I1, 10) + W%(I, I1, 0) > E%(6) Then Print #2, players_GAME$(I, I1, 0);: Print #2, Using " SCORED MORE THAN ### POINTS"; E%(6)
            If A!(I, I1, 11) > E%(7) Then Print #2, players_GAME$(I, I1, 0);: Print #2, Using " HAD MORE THAN ### REBOUNDS"; E%(7)
            If A!(I, I1, 14) > E%(8) Then Print #2, players_GAME$(I, I1, 0);: Print #2, Using " HAD MORE THAN ### ASSISTS"; E%(8)
            If A!(I, I1, 15) > E%(9) Then Print #2, players_GAME$(I, I1, 0);: Print #2, Using " BLOCKED MORE THAN ### SHOTS"; E%(9)
            If A!(I, I1, 13) > E%(10) Then Print #2, players_GAME$(I, I1, 0);: Print #2, Using " HAD MORE THAN ### STEALS"; E%(10)
            If W%(I, I1, 0) > E%(11) Then Print #2, players_GAME$(I, I1, 0);: Print #2, Using " MADE MORE THAN ### 3-PT FIELD GOALS"; E%(11)
        Next
    Next

    Print #2,

    Close #2

End Sub



'----------------------------------------
'          SAVESTATS Subroutine
'----------------------------------------
'This subroutine prompts the user in ORDER
'to determine which teams to save satts For
'(autoplay will always save both teams). 
'It also updates the GAME82 progress file, 
'since at this point only can the game be 
'considered fully complete.
Sub SAVESTATS ()

    Shared scheduleFile$

    Cls

    If autoPlay <> 1 Then

        Call TEAMBOX(1, 1)

        Do
            U$ = "SAVE STATS FOR EITHER TEAM? (Y/N)"
            Locate 7, (80 - Len(U$)) / 2
            Print U$
            Color 15, 0
            I$ = GetKeyPress$
        Loop Until UCase$(I$) = "Y" Or UCase$(I$) = "N"

        If UCase$(I$) = "Y" Then
            Cls
            Color 12: Print Tab(30); "COMPILATION OF STATS"
            Print
            Color 14: Print "(0) ";: Color 15: Print "BOTH TEAMS"
            Print
            Color 14: Print "(1) ";: Color tmRat_GAME(0, 33), tmRat_GAME(0, 34): Print gameTeams$(0); " ONLY"
            Color , 0: Print
            Color 14: Print "(2) ";: Color tmRat_GAME(1, 33), tmRat_GAME(1, 34): Print gameTeams$(1); " ONLY"
            Color , 0: Print

            Do
                J$ = GetKeyPress$
                X1 = Val(J$)
            Loop Until X1 >= 0 And X1 <= 2

        End If

    End If

    ' Update schedule, ticker, etc.
    If UCase$(I$) = "Y" Or autoPlay = 1 Then

        If _FileExists("GAME82") Then

            Call TickerSchedule

            If schedGame(1) < schedGame(0) + schedGame(2) Then
                Kill "GAME82"
                endAllGames = 1
            Else
                Open "GAME82" For Output As #2

                Print #2, scheduleFile$

                For I = 0 To 2:
                    Print #2, schedGame(I)
                Next

                Close 2

            End If

        End If

        Call COMPILESTATFILES(X1)

    End If

End Sub


'----------------------------------------
'      COMPILESTATFILES Subroutine
'----------------------------------------
'This subroutine updates the various stat 
'files for the corresponding team based on 
'the stats generated for a completed game.
Sub COMPILESTATFILES (compileOpt)

    Dim TG!

    Cls

    HH% = gameLoc

    For PX = 0 To 1

        For J = 0 To 14
            A!(PX, J, 16) = timePlayed(PX, J)
            A!(PX, J, 17) = A!(PX, J, 23)
            A!(PX, J, 18) = A!(PX, J, 24)
        Next

    Next

    For PX = 0 To 1

        For I = 0 To 14

            For J = 1 To 12
                A!(PX, I, J) = A!(PX, I, J + 6)
            Next

            W0!(PX, I) = W%(PX, I, 0)
            W1!(PX, I) = W%(PX, I, 1)

        Next

    Next

    teamIdx = 0

    Do
        C% = 0

        If autoPlay <> 1 Then
            I$ = "Y"

            If compileOpt = 1 And teamIdx = 1 Or compileOpt = 2 And teamIdx = 0 Then I$ = "N"

            If UCase$(I$) = "N" Then
                C% = 1
            Else
                Cls
            End If
        End If

        If C% <> 1 Then

            statFile$ = nameStatFile$(gameTeams$(teamIdx))

            If _FileExists(diskPaths$(1) + statFile$) Then

                Open diskPaths$(1) + statFile$ For Input As #2

                Input #2, gameTeams$(teamIdx)

                For I = 0 To 14:

                    Input #2, players_GAME$(teamIdx, I, 0)

                    For J = 0 To 13:
                        Input #2, Z2!(I, J)
                    Next J

                    Input #2, W2(teamIdx, I)
                    Input #2, W3(teamIdx, I)

                Next I

                For L = 0 To 15: Input #2, Z!(L): Next L
                For L = 0 To 15: Input #2, Z1!(L): Next L

                Close #2

            End If

            TG! = 0

            For I = 0 To 14

                If A!(teamIdx, I, 2) * 2 + A!(teamIdx, I, 4) + W0!(teamIdx, I) > Z2!(I, 13) Then Z2!(I, 13) = A!(teamIdx, I, 2) * 2 + A!(teamIdx, I, 4) + W0!(teamIdx, I)

                If A!(teamIdx, I, 10) > 0 Then Z2!(I, 0) = Z2!(I, 0) + 1

                For J = 1 To 12
                    Z2!(I, J) = Z2!(I, J) + A!(teamIdx, I, J)

                    If Not (J = 5 Or J = 6 Or J = 10 Or J = 11) Then
                        Z!(J) = Z!(J) + A!(teamIdx, I, J)
                        Z1!(J) = Z1!(J) + A!(1 - teamIdx, I, J)
                    End If
                Next J

                Z!(0) = Z!(0) + A!(teamIdx, I, 10)
                TG! = TG! + A!(teamIdx, I, 10)

            Next I

            For I = 0 To 1
                statTotals(I) = 0
                For I1 = 0 To 14
                    statTotals(I) = statTotals(I) + A!(I, I1, 5)
                Next
            Next

            Z!(15) = Z!(15) + R3(teamIdx) - statTotals(teamIdx)
            Z1!(15) = Z1!(15) + R3(1 - teamIdx) - statTotals(1 - teamIdx)

            Z!(6) = Z!(6) + TF(teamIdx)
            Z1!(6) = Z1!(6) + TF(1 - teamIdx)

            Z!(5) = Z!(5) + R3(teamIdx)
            Z1!(5) = Z1!(5) + R3(1 - teamIdx)

            Z!(11) = Z!(11) + turnovers(teamIdx)
            Z1!(11) = Z1!(11) + turnovers(1 - teamIdx)

            For J = 0 To 14:
                Z!(13) = Z!(13) + W0!(teamIdx, J)
                Z!(14) = Z!(14) + W1!(teamIdx, J)
                Z1!(13) = Z1!(13) + W0!(1 - teamIdx, J)
                Z1!(14) = Z1!(14) + W1!(1 - teamIdx, J)
                W2(teamIdx, J) = W2(teamIdx, J) + W0!(teamIdx, J)
                W3(teamIdx, J) = W3(teamIdx, J) + W1!(teamIdx, J)
            Next J

            Z!(10) = Z!(10) + score(teamIdx, 0)
            Z1!(10) = Z1!(10) + score(1 - teamIdx, 0)

            Call TEAMBOX(1, teamIdx)

            U$ = "SAVING STATS FOR " + gameTeams$(teamIdx)
            Locate , (80 - Len(U$)) / 2
            Print U$
            Color 15, 0

            Open diskPaths$(1) + statFile$ For Output As #1

            Print #1, gameTeams$(teamIdx)

            For I = 0 To 14:

                Print #1, players_GAME$(teamIdx, I, 0)

                For J = 0 To 13:
                    Print #1, Z2!(I, J)
                Next J

                Print #1, W2(teamIdx, I)
                Print #1, W3(teamIdx, I)

            Next I

            For L = 0 To 15
                Print #1, Z!(L)
            Next L

            For L = 0 To 15:
                Print #1, Z1!(L)
            Next L

            Close #1

            Y% = 0

            If _FileExists(diskPaths$(1) + statFile$ + "S") Then
                Open diskPaths$(1) + statFile$ + "S" For Input As #1
                Input #1, Y%
                For I = 1 To 100: Input #1, O%(I): Next I
                For I = 1 To 100: Input #1, HO$(I): Next I
                For I = 1 To 100: Input #1, HT%(I): Next I
                For I = 1 To 100: Input #1, H$(I): Next I
                Close #1
            End If

            If Y% < 100 Then Y% = Y% + 1

            Select Case HH%
                Case 2
                    Select Case TG!
                        Case Is < 14500
                            H$(Y%) = "N"
                        Case 14501 To 16000
                            H$(Y%) = "N1"
                        Case 16001 To 17500
                            H$(Y%) = "N2"
                        Case 17501 To 19000
                            H$(Y%) = "N3"
                        Case 19001 To 20500
                            H$(Y%) = "N4"
                    End Select

                Case Is = teamIdx
                    Select Case TG!
                        Case Is < 14500
                            H$(Y%) = "H"
                        Case 14501 To 16000
                            H$(Y%) = "H1"
                        Case 16001 To 17500
                            H$(Y%) = "H2"
                        Case 17501 To 19000
                            H$(Y%) = "H3"
                        Case 19001 To 20500
                            H$(Y%) = "H4"
                    End Select

                Case Is <> teamIdx
                    Select Case TG!
                        Case Is < 14500
                            H$(Y%) = "V"
                        Case 14501 To 16000
                            H$(Y%) = "V1"
                        Case 16001 To 17500
                            H$(Y%) = "V2"
                        Case 17501 To 19000
                            H$(Y%) = "V3"
                        Case 19001 To 20500
                            H$(Y%) = "V4"
                    End Select

            End Select

            O%(Y%) = score(teamIdx, 0)
            HO$(Y%) = gameTeams$(1 - teamIdx)
            HT%(Y%) = score(1 - teamIdx, 0)

            Open diskPaths$(1) + statFile$ + "S" For Output As #1

            Print #1, Y%

            For I = 1 To 100: Print #1, O%(I): Next I
            For I = 1 To 100: Print #1, HO$(I): Next I
            For I = 1 To 100: Print #1, HT%(I): Next I
            For I = 1 To 100: Print #1, H$(I): Next I

            Close #1

            'Reset arrays for next iteration
            For I = 1 To 100:
                O%(I) = 0
                HT%(I) = 0
                HO$(I) = ""
                H$(I) = ""
            Next I

            For J = 0 To 14
                For K = 0 To 13
                    Z2!(J, K) = 0
                Next K
            Next J

            For L = 0 To 15
                Z!(L) = 0
                Z1!(L) = 0
            Next L

        End If

        teamIdx = teamIdx + 1
        Z!(12) = 0

    Loop Until teamIdx = 2

End Sub


