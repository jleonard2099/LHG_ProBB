Sub RATINGTABLES ()

    For I = 0 To 1

        F5%(I, 0) = 0
        F5%(I, 1) = 0
        F5%(I, 2) = 0
        F5%(I, 3) = 0
        F5%(I, 4) = 0

        For I1 = 0 To 4
            F5%(I, 0) = F5%(I, 0) + A!(I, B1!(I, I1), 17)
            F5%(I, 1) = F5%(I, 1) + plyrRat_GAME(I, B1!(I, I1), 18)
            F5%(I, 2) = F5%(I, 2) + A!(I, B1!(I, I1), 25)
            F5%(I, 3) = F5%(I, 3) + A!(I, B1!(I, I1), 5)
            F5%(I, 4) = F5%(I, 4) + A!(I, B1!(I, I1), 4) / 10
        Next I1

        F5%(I, 4) = Int((F5%(I, 4) / (F7%(I, 6) * .9)) * 100 + .5)

    Next

End Sub


