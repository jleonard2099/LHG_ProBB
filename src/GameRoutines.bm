                TREC(117, 1) = I2
                TREC(117, 2) = 0
                TRC$(117, 0) = gameTeams$(I)
                TRC$(117, 1) = SITE$(I)
                TRC$(117, 2) = gameTeams$(1 - I)
                TRC$(117, 3) = Date$
            End If
        Next

        ' BLOCKS
        If V8(I, 8) > TREC(118, 0) Then
            TREC(118, 0) = V8(I, 8)
            TREC(118, 1) = 0
            TREC(118, 2) = 0
            TRC$(118, 0) = gameTeams$(I)
            TRC$(118, 1) = SITE$(I)
            TRC$(118, 2) = gameTeams$(1 - I)
            TRC$(118, 3) = Date$
        End If

        If V8(I, 8) + V8(1 - I, 8) > TREC(119, 0) Then
            TREC(119, 0) = V8(I, 8) + V8(1 - I, 8)
            TREC(119, 1) = V8(I, 8)
            TREC(119, 2) = V8(1 - I, 8)
            TRC$(119, 0) = gameTeams$(I)
            TRC$(119, 1) = SITE$(I)
            TRC$(119, 2) = gameTeams$(1 - I)
            TRC$(119, 3) = Date$
        End If

        ' TURNOVERS
        If V8(I, 12) > TREC(122, 0) Then
            TREC(122, 0) = V8(I, 12)
            TREC(122, 1) = 0
            TREC(122, 2) = 0
            TRC$(122, 0) = gameTeams$(I)
            TRC$(122, 1) = SITE$(I)
            TRC$(122, 2) = gameTeams$(1 - I)
            TRC$(122, 3) = Date$
        End If

        If V8(I, 12) < TREC(123, 0) Then
            TREC(123, 0) = V8(I, 12)
            TREC(123, 1) = 0
            TREC(123, 2) = 0
            TRC$(123, 0) = gameTeams$(I)
            TRC$(123, 1) = SITE$(I)
            TRC$(123, 2) = gameTeams$(1 - I)
            TRC$(123, 3) = Date$
        End If
        If V8(I, 12) + V8(1 - I, 12) > TREC(124, 0) Then
            TREC(124, 0) = V8(I, 12) + V8(1 - I, 12)
            TREC(124, 1) = V8(I, 12)
            TREC(124, 2) = V8(1 - I, 12)
            TRC$(124, 0) = gameTeams$(I)
            TRC$(124, 1) = SITE$(I)
            TRC$(124, 2) = gameTeams$(1 - I)
            TRC$(124, 3) = Date$
        End If

        If V8(I, 12) + V8(1 - I, 12) < TREC(125, 0) Then
            TREC(125, 0) = V8(I, 12) + V8(1 - I, 12)
            TREC(125, 1) = V8(I, 12)
            TREC(125, 2) = V8(1 - I, 12)
            TRC$(125, 0) = gameTeams$(I)
            TRC$(125, 1) = SITE$(I)
            TRC$(125, 2) = gameTeams$(1 - I)
            TRC$(125, 3) = Date$
        End If

    Next

    Open diskPaths$(1) + "RECORDS.GAM" For Output As #1

    For I = 0 To 48
        For I1 = 0 To 2
            Write #1, indRecords!(I, I1)
        Next
        For I1 = 0 To 4
            Write #1, RC$(I, I1)
        Next
    Next

    For I = 0 To 125
        For I1 = 0 To 2
            Write #1, TREC(I, I1)
        Next
        For I1 = 0 To 3
            Write #1, TRC$(I, I1)
        Next
    Next

    Close 1

